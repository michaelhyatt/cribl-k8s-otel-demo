{
  "id": "cribl-tcp",
  "systemFields": [
    "cribl_pipe"
  ],
  "streamtags": [],
  "loadBalanced": false,
  "compression": "gzip",
  "logFailedRequests": false,
  "throttleRatePerSec": "0",
  "tls": {
    "disabled": true,
    "rejectUnauthorized": true
  },
  "connectionTimeout": 10000,
  "writeTimeout": 60000,
  "tokenTTLMinutes": 60,
  "excludeFields": [
    "__kube_*",
    "__metadata",
    "__winEvent"
  ],
  "onBackpressure": "block",
  "port": 10300,
  "excludeSelf": false,
  "hosts": [
    {
      "host": "cribl-worker-logstream-workergroup",
      "port": 10300,
      "tls": "inherit",
      "weight": 1
    }
  ],
  "dnsResolvePeriodSec": 600,
  "loadBalanceStatsPeriodSec": 300,
  "maxConcurrentSenders": 0,
  "host": "cribl-worker-logstream-workergroup",
  "pqCompress": "none",
  "pqMaxFileSize": "1 MB",
  "pqMaxSize": "5GB",
  "pqMode": "error",
  "pqOnBackpressure": "block",
  "pqPath": "$CRIBL_HOME/state/queues",
  "type": "cribl_tcp"
}